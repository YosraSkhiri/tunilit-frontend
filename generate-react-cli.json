{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "index": "./templates/index.tsx",
        "component": "./templates/TemplateName.tsx",
        "story": "./templates/TemplateName.stories.tsx",
        "style": "./templates/TemplateName.module.scss",
        "test": "./templates/TemplateName.test.tsx",
        "types": "./templates/TemplateName.types.ts"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "withIndex": true,
      "withTypes": true
    }
  }
}